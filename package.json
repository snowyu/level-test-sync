{
  "name": "level-test-sync",
  "description": "get a clean levelup-sync instance for testing.",
  "version": "1.6.6",
  "homepage": "https://github.com/snowyu/level-test-sync",
  "repository": {
    "type": "git",
    "url": "git://github.com/snowyu/level-test-sync.git"
  },
  "dependencies": {
    "level-js": "~2.1.6",
    "rimraf": "~2.2.6",
    "osenv": "0.1.0",
    "memdown": "~0.10.2",
    "levelup-sync": "~0.19.2",
    "xtend": "~4.0.0",
    "mkdirp": "~0.5.0"
  },
  "optionalDependencies": {
    "leveldown-sync": "~1.0.1"
  },
  "devDependencies": {
    "tape": "~3.0.0"
